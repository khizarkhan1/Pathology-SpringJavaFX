<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.springjavafx.controllers.LoginController">
   <children>
      <TextField fx:id="userField" layoutX="219.0" layoutY="141.0" promptText="Username" />
      <TextField fx:id="passwordField" layoutX="219.0" layoutY="187.0" promptText="Password" />
      <Label layoutX="140.0" layoutY="145.0" prefHeight="18.0" prefWidth="67.0" text="Username" />
      <Label layoutX="143.0" layoutY="191.0" text="Password" />
      <Button layoutX="258.0" layoutY="251.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="26.0" prefWidth="84.0" text="Login" />
      <Label fx:id="usernameErrorLabel" layoutX="219.0" layoutY="166.0" prefHeight="17.0" prefWidth="149.0" textFill="RED" />
      <Label fx:id="passwordErrorLabel" layoutX="219.0" layoutY="212.0" prefHeight="17.0" prefWidth="149.0" textFill="RED" />
   </children>
</AnchorPane>
